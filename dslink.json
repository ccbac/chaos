{
  "name": "chaos",
  "version": "1.0.0",
  "description": "Generator of random and ordered data.",
  "license": "Apache",
  "author": {
    "name": "YOUR NAME",
    "email": "YOUR EMAIL"
  },
  "main": "bin/chaos",
  "repository": {
    "type": "git",
    "url": "https://github.com/ccbac/chaos"
  },
  "bugs": {
    "url": "https://github.com/ccbac/chaos/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "chaos"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "com.ccbac.chaos.Main"
    }
  }
}
